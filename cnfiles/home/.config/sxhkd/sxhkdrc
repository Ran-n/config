# abrir terminal
control + alt + t
	alacritty

# abrir programas
super+o;{b,c,r,v,d}
	{brave,$TERMINAL -e calcurse,$TERMINAL -e ranger,$TERMINAL -e nvim,dmenu_run}

# vim
super + v
	$TERMINAL -e nvim

super + c
	$TERMINAL -e calcurse

super + r
	$TERMINAL -e ranger

super + b
	brave

super + d
	dmenu_run

super + b
	brave

super + s
	sqlitebrowser
